for x in CC CXX KCC ; do
	if eval '[ "$SDECFG_DEFAULT_'$x'" = "icc" ]' ; then
		if [ -x "`type -p icc`" ]; then
			eval 'export '$x'="icc"'
			for x in $base/package/*/icc/patches/pkg_${pkg}{,_*}.patch
			do
				if [ -f $x ] ; then
					var_remove patchfiles " " $x
					var_append patchfiles " " $x
				fi
			done
		fi
	fi
done
