# Filter out the access of the module registration files. We could force it to
# just python instances via \.python\. - but just the ext should be fine.
var_append flistrfilter '|' '.*:	.*\.(pth|egg)'
#var_apddpend flistrfilter '|' '.*:	.*__pycache__.*\.pyc'
var_append flistdel '|' '.*/__pycache__.*'
var_append flistdel '|' '.*easy-install\.pth'
