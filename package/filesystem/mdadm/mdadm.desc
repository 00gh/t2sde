# STRIP is used inside the Makefile to pass the '-s' option to the
# install command (while it should be the name of the strip binary,
# actually...)
unset STRIP
# breaking strict aliasing rules..
var_append GCC_WRAPPER_APPEND ' ' -fno-strict-aliasing
var_append makeopt ' ' RUN_DIR=/var/run/mdadm
var_append makeinstopt ' ' RUN_DIR=/var/run/mdadm

