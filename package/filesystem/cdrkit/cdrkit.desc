cdrkit_pm(){
	ln -sf wodim $root$bindir/cdrecord
	ln -sf wodim.1 $root$mandir/man1/cdrecord.1
	ln -sf genisoimage $root$bindir/mkisofs
	ln -sf genisoimage.1 $root$mandir/man1/mkisofs.1
}
